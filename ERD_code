dept_emp
-
emp_no INT FK >- employees.emp_no
dept_no VARCHAR(10) FK - departments.dept_no
from_date DATE
to_date DATE

departments
-
dept_no PK VARCHAR(10)
dept_name VARCHAR(30)

dept_manager
-
emp_no INT FK - employees.emp_no
dept_no VARCHAR(10) FK - departments.dept_no
from_date DATE
to_date DATE

employees
-
emp_no PK INT
birth_date DATE
first_name VARCHAR(20)
last_name VARCHAR(20)
gender VARCHAR(1)
hire_date DATE

salaries
-
emp_no INT FK - employees.emp_no
salary INT
from_date DATE
to_date DATE

titles
-
emp_no INT FK >- employees.emp_no
title VARCHAR(20)
from_date DATE
to_date DATE
